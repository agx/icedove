# The embedded libjpeg is libjpeg-turbo, not libjpeg.
icedove: embedded-library usr/lib/icedove/libxul.so: libjpeg
# Upstream code is not ready to use system library
icedove: embedded-library usr/lib/icedove/libxul.so: srtp
# The embedded libpng is patched for APNG support
icedove: embedded-library usr/lib/icedove/libxul.so: libpng
# we use internal librarys due to old version in jessie
icedove: embedded-library usr/lib/icedove/libnss3.so: nss
