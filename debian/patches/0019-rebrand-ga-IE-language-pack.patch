 4 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.dtd b/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.dtd
index 0121e8b..120541a 100644
--- a/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.dtd
+++ b/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.dtd
@@ -1,4 +1,4 @@
-<!ENTITY brandShortName "Thunderbird">
-<!ENTITY brandFullName "Mozilla Thunderbird">
+<!ENTITY brandShortName "Icedove">
+<!ENTITY brandFullName "Icedove">
 <!ENTITY vendorShortName "Mozilla">
-<!ENTITY logoTrademark "Is trádmharcanna de chuid Fondúireacht Mozilla iad Mozilla Thunderbird agus suaitheantais Thunderbird.">
+<!ENTITY logoTrademark " ">
diff --git a/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.properties b/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.properties
index 857d0c3..44bf191 100644
