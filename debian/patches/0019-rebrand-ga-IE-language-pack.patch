+++ b/upstream/ga-IE/chrome/locale/ga-IE/branding/brand.properties
@@ -1,3 +1,3 @@
-brandShortName=Thunderbird
-brandFullName=Mozilla Thunderbird
+brandShortName=Icedove
+brandFullName=Icedove
 vendorShortName=Mozilla
diff --git a/upstream/ga-IE/chrome/locale/ga-IE/messenger/accountCreation.dtd b/upstream/ga-IE/chrome/locale/ga-IE/messenger/accountCreation.dtd
index 7d02ddf..a51e90c 100644
