From: Christoph Goehre <chris@sigxcpu.org>
Date: Sun, 21 Feb 2010 17:00:25 +0100
Subject: [PATCH] rebrand sq language pack

---
 upstream/sq/chrome/locale/sq/branding/brand.dtd    |    9 ++++-----
 .../sq/chrome/locale/sq/branding/brand.properties  |    6 +++---
 .../chrome/locale/sq/messenger/accountCreation.dtd |    2 +-
 upstream/sq/chrome/locale/sq/messenger/credits.dtd |    2 +-
 4 files changed, 9 insertions(+), 10 deletions(-)

diff --git a/upstream/sq/chrome/locale/sq/branding/brand.dtd b/upstream/sq/chrome/locale/sq/branding/brand.dtd
index eb78545..e1e28fc 100644
--- a/upstream/sq/chrome/locale/sq/branding/brand.dtd
+++ b/upstream/sq/chrome/locale/sq/branding/brand.dtd
@@ -1,5 +1,4 @@
-<!ENTITY  brandShortName        "Thunderbird">
-<!ENTITY  brandFullName         "Mozilla Thunderbird">
-<!ENTITY  vendorShortName       "Mozilla Thunderbird">
-<!ENTITY  logoTrademark         "Stemat e Mozilla Thunderbird-it dhe Thunderbird-it
-                                 janë shenja tregtare të Mozilla Foundation-it.">
+<!ENTITY  brandShortName        "Icedove">
+<!ENTITY  brandFullName         "Icedove">
+<!ENTITY  vendorShortName       "Mozilla">
+<!ENTITY  logoTrademark         " ">
diff --git a/upstream/sq/chrome/locale/sq/branding/brand.properties b/upstream/sq/chrome/locale/sq/branding/brand.properties
index b31a75c..44bf191 100644
--- a/upstream/sq/chrome/locale/sq/branding/brand.properties
+++ b/upstream/sq/chrome/locale/sq/branding/brand.properties
@@ -1,3 +1,3 @@
-brandShortName=Thunderbird
-brandFullName=Mozilla Thunderbird
-vendorShortName=Mozilla Thunderbird
+brandShortName=Icedove
+brandFullName=Icedove
+vendorShortName=Mozilla
diff --git a/upstream/sq/chrome/locale/sq/messenger/accountCreation.dtd b/upstream/sq/chrome/locale/sq/messenger/accountCreation.dtd
index d624be7..13a6555 100644
--- a/upstream/sq/chrome/locale/sq/messenger/accountCreation.dtd
+++ b/upstream/sq/chrome/locale/sq/messenger/accountCreation.dtd
@@ -48,7 +48,7 @@
 <!ENTITY changeSettings.label            "Ndryshoni Rregullimet">
 <!ENTITY changeSettings.accesskey        "N">
 
-<!ENTITY contactYourProvider.description "&brandShortName;-i ju lejon të mbërrini te posta juaj duke përdorur formësime të gatshme. Sidoqoftë, lidhur me këto lidhje të pasakta, do të duhej të lidheshit me përgjegjësin ose mundësuesin e email-it. Për më tepër të dhëna, shihni FAQ për Thunderbird-in.">
+<!ENTITY contactYourProvider.description "&brandShortName;-i ju lejon të mbërrini te posta juaj duke përdorur formësime të gatshme. Sidoqoftë, lidhur me këto lidhje të pasakta, do të duhej të lidheshit me përgjegjësin ose mundësuesin e email-it. Për më tepër të dhëna, shihni FAQ për Icedove-in.">
 
 <!ENTITY insecureServer.tooltip.title    "Kujdes! Ky është shërbyes i pasigurt.">
 <!ENTITY insecureServer.tooltip.details  "Për më tepër hollësi klikoni te rrethi.">
diff --git a/upstream/sq/chrome/locale/sq/messenger/credits.dtd b/upstream/sq/chrome/locale/sq/messenger/credits.dtd
index ab3b7ee..22825c8 100644
--- a/upstream/sq/chrome/locale/sq/messenger/credits.dtd
+++ b/upstream/sq/chrome/locale/sq/messenger/credits.dtd
@@ -1,5 +1,5 @@
 <!ENTITY credit.title     "&brandFullName;-i u punua nga">
-<!ENTITY brandMotto       "Thunderbirds Are Go!">
+<!ENTITY brandMotto       "Icedove Are Go!">
 <!ENTITY credit.thanks    "Falënderime të Veçanta Për">
 
