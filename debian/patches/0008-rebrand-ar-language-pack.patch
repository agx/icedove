From: Christoph Goehre <chris@sigxcpu.org>
Date: Sun, 21 Feb 2010 12:11:46 +0100
Subject: [PATCH] rebrand ar language pack

---
 upstream/ar/chrome/locale/ar/branding/brand.dtd    |    9 ++++-----
 .../ar/chrome/locale/ar/branding/brand.properties  |    6 +++---
 2 files changed, 7 insertions(+), 8 deletions(-)

diff --git a/upstream/ar/chrome/locale/ar/branding/brand.dtd b/upstream/ar/chrome/locale/ar/branding/brand.dtd
index c77926f..e1e28fc 100644
--- a/upstream/ar/chrome/locale/ar/branding/brand.dtd
+++ b/upstream/ar/chrome/locale/ar/branding/brand.dtd
@@ -1,5 +1,4 @@
-<!ENTITY  brandShortName        "ثنْدَربِرد">
-<!ENTITY  brandFullName         "موزيلا ثنْدَربِرد">
-<!ENTITY  vendorShortName       "موزيلا">
-<!ENTITY  logoTrademark         "موزيلا ثَنْدربِرد وشعارات ثَنْدربِرد 
-                                 علامات تجارية لمؤسسة موزيلا.">
+<!ENTITY  brandShortName        "Icedove">
+<!ENTITY  brandFullName         "Icedove">
+<!ENTITY  vendorShortName       "Mozilla">
+<!ENTITY  logoTrademark         " ">
diff --git a/upstream/ar/chrome/locale/ar/branding/brand.properties b/upstream/ar/chrome/locale/ar/branding/brand.properties
index a7afa70..44bf191 100644
--- a/upstream/ar/chrome/locale/ar/branding/brand.properties
+++ b/upstream/ar/chrome/locale/ar/branding/brand.properties
@@ -1,3 +1,3 @@
-brandShortName=ثنْدَربِرد
-brandFullName=موزيلا ثنْدَربِرد
-vendorShortName=موزيلا
+brandShortName=Icedove
+brandFullName=Icedove
+vendorShortName=Mozilla
