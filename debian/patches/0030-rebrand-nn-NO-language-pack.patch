From: Christoph Goehre <chris@sigxcpu.org>
Date: Sun, 21 Feb 2010 14:49:04 +0100
Subject: [PATCH] rebrand nn-NO language pack

---
 .../nn-NO/chrome/locale/nn-NO/branding/brand.dtd   |    7 +++----
 .../chrome/locale/nn-NO/branding/brand.properties  |    4 ++--
 .../locale/nn-NO/messenger/accountCreation.dtd     |    2 +-
 .../chrome/locale/nn-NO/messenger/credits.dtd      |    2 +-
 4 files changed, 7 insertions(+), 8 deletions(-)

diff --git a/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.dtd b/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.dtd
index 21d009c..e1e28fc 100644
--- a/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.dtd
+++ b/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.dtd
@@ -1,5 +1,4 @@
-<!ENTITY  brandShortName        "Thunderbird">
-<!ENTITY  brandFullName         "Mozilla Thunderbird">
+<!ENTITY  brandShortName        "Icedove">
+<!ENTITY  brandFullName         "Icedove">
 <!ENTITY  vendorShortName       "Mozilla">
-<!ENTITY  logoTrademark         "Mozilla Thunderbird og Thunderbird-logoane
-                                 er varemerke som tilhøyrer Mozilla Foundation.">
+<!ENTITY  logoTrademark         " ">
diff --git a/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.properties b/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.properties
index 857d0c3..44bf191 100644
--- a/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.properties
+++ b/upstream/nn-NO/chrome/locale/nn-NO/branding/brand.properties
@@ -1,3 +1,3 @@
-brandShortName=Thunderbird
-brandFullName=Mozilla Thunderbird
+brandShortName=Icedove
+brandFullName=Icedove
 vendorShortName=Mozilla
diff --git a/upstream/nn-NO/chrome/locale/nn-NO/messenger/accountCreation.dtd b/upstream/nn-NO/chrome/locale/nn-NO/messenger/accountCreation.dtd
index 44f3821..14676a6 100644
--- a/upstream/nn-NO/chrome/locale/nn-NO/messenger/accountCreation.dtd
+++ b/upstream/nn-NO/chrome/locale/nn-NO/messenger/accountCreation.dtd
@@ -68,7 +68,7 @@
 <!ENTITY changeSettings.label            "Endre innstillingar">
 <!ENTITY changeSettings.accesskey        "E">
 
-<!ENTITY contactYourProvider.description "&brandShortName; kan la deg laste ned e-post med dei gitte konfigurasjonane. Men du bør kontakte systemansvarleg eller e-posttilbydaren om desse feilaktige tilkoplingane. Sjå Thunderbird FAQ for meir informasjon.">
+<!ENTITY contactYourProvider.description "&brandShortName; kan la deg laste ned e-post med dei gitte konfigurasjonane. Men du bør kontakte systemansvarleg eller e-posttilbydaren om desse feilaktige tilkoplingane. Sjå Icedove FAQ for meir informasjon.">
 
 <!ENTITY insecureServer.tooltip.title    "Åtvaring! Dette er ein utrygg tenar.">
 <!ENTITY insecureServer.tooltip.details  "Click circle for more details.">
diff --git a/upstream/nn-NO/chrome/locale/nn-NO/messenger/credits.dtd b/upstream/nn-NO/chrome/locale/nn-NO/messenger/credits.dtd
index 28682d7..32265b9 100644
--- a/upstream/nn-NO/chrome/locale/nn-NO/messenger/credits.dtd
+++ b/upstream/nn-NO/chrome/locale/nn-NO/messenger/credits.dtd
@@ -1,5 +1,5 @@
 <!ENTITY credit.title     "&brandFullName; bidragsytarar">
-<!ENTITY brandMotto       "Thunderbirds Are Go!">
+<!ENTITY brandMotto       "Icedove Are Go!">
 <!ENTITY credit.thanks    "Spesielt takk til">
 
