topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE = suitefeeds
LIBRARY_NAME = suitefeeds_s
FORCE_STATIC_LIB = 1

DEFINES += \
	-DMOZ_APP_NAME=$(MOZ_APP_NAME) \
	-DMOZ_MACBUNDLE_NAME=$(MOZ_MACBUNDLE_NAME) \
